{
  "name": "vite-plugin-auto-export",
  "version": "0.0.1",
  "description": "指定文件夹内的数据自动导出，适用于i18n的国际化配置",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build-bundle",
    "build-bundle": "esbuild src/index.ts --bundle --platform=node --target=node16 --external:@vue/compiler-sfc --external:vue/compiler-sfc --external:vite --outfile=dist/index.js"
  },
  "keywords": [],
  "author": "lxl",
  "license": "ISC",
  "peerDependencies": {
    "vite": "5.2.11"
  },
  "dependencies": {
    "@types/node": "^20.12.12",
    "esbuild": "^0.21.3"
  }
}
